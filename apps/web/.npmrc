# Performance optimizations for npm
package-lock=true
save-exact=true
engine-strict=true
audit=false
fund=false
progress=false
loglevel=error

# Skip optional dependencies
optional=false

# Prefer offline for faster installs
prefer-offline=true

# Speed up installs
fetch-retries=2
fetch-retry-factor=10
fetch-retry-mintimeout=10000
fetch-retry-maxtimeout=60000
